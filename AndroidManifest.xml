<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.appspot.bgje0341.petagram">

    <application
        android:allowBackup="true"
        android:icon="@drawable/huella_de_pata_de_animal"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".fab_mas"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.TranslucentSystemBars"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.appspot.bgje0341.petagram.MainActivity" />
        </activity>
    </application>

</manifest>
